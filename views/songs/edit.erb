<h1>Edit Song</h1>
<form class="" action="/songs/:id" method="post">
  <div class="">
    <label>Title</label>
    <input type="text" name="title" value="<%= @song[:title] %>">
  </div>
  <div class="">
    <label>Body</label>
    <input type="text" name="body" value="<%= @song[:body] %>">
  </div>
  <div class="">
    <input type="submit" name="_method" value="PUT">
  </div>

</form>
